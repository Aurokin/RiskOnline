<div id ="regionInformation" style="float: right; z-index:10; position: relative; left: -450px; width: 300px; min-height: 600px; background-color: #FAFFBD; border-radius: 20px;padding:10px;">
	<div class="panelHead">
		<h2 id="regionName"></h2>
	</div>
	<div class="panelBody">
		<b>Controlled By: </b><span id="regionCB"></span><br>
		<b>Number Of Armies: </b><span id="regionArmies"></span>
		<div class="hidden" id="regionID"></div>
		<div class="hidden" id="regionNameDB"></div>
	</div>
	<div class="panelFooter">
		<button id="placeArmyBtn" type="button" class="disabled" disabled>Place Army</button>
		<button id="moveArmyBtn" type="button" class="disabled" disabled>Move Army</button>
		<button id="attackArmyBtn" type="button" class="disabled" disabled>Attack Army</button>
	</div>
</div>
<div id ="information" style="float: right; z-index:10; position: relative; left: 200px; width: 300px; min-height: 600px; background-color: #FAFFBD; border-radius: 20px;padding:10px;">
	<div class="panelHead">
		<h2 id="gameName"></h2>
	</div>
	<div class="panelBody">
		<b>Current Users Turn: </b><span id="userTurn"></span><br>
		<b>Current Round: </b><span id="currentRound"></span><br>
		<b>Current Phase: </b><span id="currentPhase"></span><br>
		<b>Remaining Armies: </b><span id="remainingArmies"></span><br>
		<b>Players</b><br>
		<div id="playersInGame">

		</div>
	</div>
	<div class="panelFooter">
		<button id="endPhaseBtn" type="button" class="disabled" disabled>End Phase</button>
		<button id="endTurnBtn" type="button" class="disabled" disabled>End Turn</button>
	</div>
</div>
<div id="chatContainer">
	<div id="chatGL" class="clearfix">
	</div>
	<form id="chatForm">
		<input id="chatInput" autocomplete="off" /><button>Send</button>
	</form>
</div>

